<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.felix.mapper.SysLoginLogMapper">

    <resultMap id="SysLoginLogMap" type="org.felix.model.entity.SysLoginLog">
        <result property="id" column="id" jdbcType="VARCHAR"/>
        <result property="createTime" column="create_time" jdbcType="TIMESTAMP"/>
        <result property="userId" column="user_id" jdbcType="VARCHAR"/>
        <result property="loginIp" column="login_ip" jdbcType="VARCHAR"/>
        <result property="loginAddress" column="login_address" jdbcType="VARCHAR"/>
        <result property="loginBrowser" column="login_browser" jdbcType="VARCHAR"/>
        <result property="loginWay" column="login_way" jdbcType="INTEGER"/>
        <result property="userAgent" column="user_agent" jdbcType="VARCHAR"/>
    </resultMap>
    <sql id="Base_Column_List">
        id, create_time, user_id, login_ip, login_address, login_browser, login_way, user_agent
    </sql>

    <insert id="insertSelective" parameterType="org.felix.model.entity.SysLoginLog">
        insert into sys_login_log
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="id != null">
                id,
            </if>
            <if test="createTime != null">
                create_time,
            </if>
            <if test="userId != null">
                user_id,
            </if>
            <if test="loginIp != null">
                login_ip,
            </if>
            <if test="loginAddress != null">
                login_address,
            </if>
            <if test="loginBrowser != null">
                login_browser,
            </if>
            <if test="loginWay != null">
                login_way,
            </if>
            <if test="userAgent != null">
                user_agent,
            </if>
        </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="id != null">
                #{id,jdbcType=VARCHAR},
            </if>
            <if test="createTime != null">
                #{createTime,jdbcType=TIMESTAMP},
            </if>
            <if test="userId != null">
                #{userId,jdbcType=VARCHAR},
            </if>
            <if test="loginIp != null">
                #{loginIp,jdbcType=VARCHAR},
            </if>
            <if test="loginAddress != null">
                #{loginAddress,jdbcType=VARCHAR},
            </if>
            <if test="loginBrowser != null">
                #{loginBrowser,jdbcType=VARCHAR},
            </if>
            <if test="loginWay != null">
                #{loginWay,jdbcType=INTEGER},
            </if>
            <if test="userAgent != null">
                #{userAgent,jdbcType=VARCHAR},
            </if>
        </trim>
    </insert>

    <select id="selectAllLog" resultType="org.felix.model.entity.ExportLoginLogBean">
         SELECT
            t1.id,
            t1.create_time,
            t1.user_id,
            t1.login_ip,
            t1.login_address,
            t1.login_browser,
            t1.login_way,
            t2.username AS userName
        FROM
            sys_login_log t1
                LEFT JOIN
            sys_user t2 ON t1.user_id = t2.id
        ORDER BY t1.create_time DESC
    </select>

    <select id="selectAll" resultType="org.felix.model.vo.resp.LoginLogVO">
        SELECT
            t1.id,
            t1.create_time,
            t1.user_id,
            t1.login_ip,
            t1.login_address,
            t1.login_browser,
            t1.login_way,
            t2.username AS userName
        FROM
         sys_login_log t1
        LEFT JOIN
         sys_user t2 ON t1.user_id = t2.id
        <where>
            <if test="username != null and username != ''">
                AND t2.username LIKE concat('%',#{username},'%')
            </if>
            <if test="startTime != null and startTime != ''">
                AND t1.create_time &gt;= #{startTime}
            </if>
            <if test="endTime != null and endTime != ''">
                AND t1.create_time &lt;= #{endTime}
            </if>
        </where>
        order by t1.create_time desc
    </select>

    <delete id="batchDeletedLog">
        delete
        from sys_login_log
        where id in
        <foreach item="item" collection="list" open="(" separator="," close=")">
            #{item}
        </foreach>
    </delete>


</mapper>
